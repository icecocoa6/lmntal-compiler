{module(lmnguardcompiler).

	% 1st Phase
	lmnguardcompiler.phase1 :-
	
		lmnguardcompiler.compile,
		
		( lmnguardcompiler.compile, guard({$guard}) :-
			lmnguardcompiler.compile(G), guard({'+'(G), $guard}) ),

	%( R=lmnguardcompiler.compile({$guard}) :- () ),
	
		().

	% 2nd Phase
	lmnguardcompiler.phase2 :-
	( lmnguardcompiler.compile({$guard}) :- {$guard} ),
	
	().

}.