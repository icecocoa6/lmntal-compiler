timer:benchmark(X,"Elapsed Time"), {+(X).
    go :- thread.create(8).
    generate(10000).
    generate(X), thread.num(N) :- Y = X / N | thread.num(N), thread.all({generate(Y)}).
    thread.all({gen@@ generate(X) :- X>0,Y=X-1 | n(integer.rnd(100)), generate(X-1)}).
    thread.all({generate(0):-.}).
    thread.all({norm(0)}).
    thread.all({cal@@ norm(Y), n(X) :- int(X),Z=X*X+Y | norm(Z)}).
    norm@@  norm(N), norm(M) :- norm(N+M).
}.
