createthread(4).
createthread(Num) :- Num>0 |
	createthread(Num-1),
	thread({ 
		generate(500).
		generate(X) :- X>0 | n(integer.rnd(100)), generate(X-1).
		norm(0).
		norm(Y),n(X) :- int(X) | norm(X*X+Y).
	}@"localhost").
{generate(0),norm(X),$p,@p}, {generate(0),norm(Y),$q,@q}
	:- int(X),int(Y) | {generate(0),norm(X+Y),$p,$q,@p}.