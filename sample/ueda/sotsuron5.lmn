track(uw, {+f,+y,+s,+n,+m,+k},20).
track(fys,{+u,+w,+n,+m,+k,+k},21).
track(nmk,{+u,+w,+f,+y,+s},19).
groups([(u,3),(u,4),(w,3),(w,4),(f,3),(f,4),(y,4),(y,4),
        (s,3),(s,4),(n,4),(n,4),(m,3),(m,4),(k,3),(k,3),(k,3)]).

track(A,{+D,$p},L), groups([(X,N)|Gs]) :-
   unary(A), D=X, L>=N, L1=L-N |
   session(A,X,N), track(A,{$p},L1), groups(Gs).
   