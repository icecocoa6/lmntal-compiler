//クラス定義
{
class(onepb).//クラス名宣言

buf={empty}.//フィールド

//メソッド定義
put(S), buf={empty} :- buf={contains(S)}.
get(), buf={contains(S)} :- ret(S), buf={empty}.
}.

buf1=new(onepb).
buf2=new(onepb).

buf1#put(1).
buf1#put(2).
buf2#put(3).
buf2#put(4).

ret1=buf1#get().
ret2=buf1#get().
ret3=buf2#get().
ret4=buf2#get().