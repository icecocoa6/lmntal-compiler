scanner=scanner.new(sys.in).
//scanner=scanner.new(fis.new(file.new("to_the_max.txt"))).

start.

//nを読み込む
start :- r1=scanner->nextInt(n).
r1(nil) :- sys.exit(1).

n(N) :- int(N) | nn(N*N).

//行列の値をn*n個読み込む
matrix=[].
nn(NN) :- NN > 0 | r2=scanner->nextInt(data), nn(NN-1).
//data(Data), matrix(Z) :- matrix=list.append(Z,[Data]).
data(Data), matrix(Z) :- matrix=[Data|Z].