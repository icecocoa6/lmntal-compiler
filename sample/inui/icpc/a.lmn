H=is_prime(N) :- N mod 2 =\= 0 | H=is_prime(N, 3, math.sqrt(N)).
H=is_prime(N, M, Nsqrt) :- M  > Nsqrt, int(N) | H=true.
H=is_prime(N, M, Nsqrt) :- M =< Nsqrt, N mod M =:= 0 | H=false.
H=is_prime(N, M, Nsqrt) :- M =< Nsqrt, N mod M =\= 0 | H=is_prime(N, M+2, Nsqrt).

