scanner.use.
integer.use.
io.use.
if.use.

next_set.

//データ入力
next_set :- ra=scanner.nextInt(a).
ra(done) :- rb=scanner.nextInt(d).
rb(done) :- rc=scanner.nextInt(n).

//データセットの終了判定
rc(done), a(0), d(0), n(0) :- .
rc(done), a(A), d(D), n(N) :- int(A), int(D), int(N) | a(A), d(D), n(N).

//素数の判定
H=is_prime(1) :- H=false.
H=is_prime(2) :- H=true.
H=is_prime(N) :- N mod 2 =:= 0 | H=false.
