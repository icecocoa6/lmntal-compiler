/* オブジェクトをコピー・破棄するバージョン*/
{
module(point).
H=point.new(X, Y) :- int(X), int(Y) | H={x(X), y(Y)}.
H=setX({x(X), $p}, NewX) :- int(X), int(NewX) | H={x(NewX), $p}.
H=setY({y(Y), $p}, NewY) :- int(Y), int(NewY) | H={y(NewY), $p}.
H=getX({x(X), $p}, ResX) :- int(X) | H={x(X), $p}, ResX=X.
H=getY({y(Y), $p}, ResY) :- int(Y) | H={y(Y), $p}, ResY=Y.
H=translate(Point, DX, DY) :- H=setY(getY(setX(getX(Point, X), X+DX), Y), Y+DY).
}.

