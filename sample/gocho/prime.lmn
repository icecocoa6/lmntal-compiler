% 
% 素数を求めるプログラム。

{
  n(31). p([2]).
  ini @@ n(N)	:- M=N+1 | n(3,M).
  inc @@ n(M, N), F=[P |R]	:-	M<N, L=M+1, (M mod P)=:= 0		|
         n(L, N), F=[P |R].
}.

pri @@ {n(M, N), p([P |R]),$p[R],@p}/ :- int(P), M<N, L=M+1 |
       {n(L, N), p([M,P |R]),$p[R],@p}.
end @@ {n(M, N), $p[], @p}/ :- M=:=N | $p[].



