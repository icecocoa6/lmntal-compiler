n(8).
n(12).
n(5).

init@@  n(X), n(Y)	:- X>=Y	|
        lcm(X, Y).
lcm1@@	lcm(X,Y)    :- X>=Y, P=X*Y |
		    gcd(X,Y), lcm(P).
gcd1@@  gcd(X,Y)    :- (X mod Y)=\=0, X2=Y,	Y2=X mod Y	|
		    gcd(Y,Y2).		
gcd2@@	gcd(X, Y)		:- (X mod Y)=:=0  |
        gcd(Y).
lcm2@@  gcd(X), lcm(Y)	:- D = Y / X	|
        n(D).
		