# マージ・配分ルーチン

{
I=[D|L],{data(nil),i(I),$p} :- 
	string(D),\+($p=(o2(X),$pp)) | I=L,{data(D),i(I),$p}.
{data(D),o(O),$p} :- 
	string(D) | {data(D),o2(O2),$p},O=[D|O2].
{data(D),$p} :-
	string(D),\+($p=(o(X),$pp)) | {data(nil),$p}.
{data(nil),o2(O),$p} :- {data(nil),o(O),$p}.
